	<munit:test name="UpdateDocumentWithJsonFilePathFlowTest2" expectedErrorType="ANY"
		description="Test" doc:id="7415848d-ad3c-47d1-849b-d06cc7b2c952" timeOut="600000">
		<munit:execution>
			<elasticsearch:index-document doc:name="Document - Index"
				doc:id="51122d43-8c06-465f-8547-e133deb70dd6" config-ref="Elasticsearch_HTTP_Config3"
				index="update_doc_index_1" documentId="update_doc_id_1">
				<elasticsearch:document-sources>
					<elasticsearch:document-source key="key1"
						value="value1" />
					<elasticsearch:document-source key="key2"
						value="value2" />
				</elasticsearch:document-sources>
			</elasticsearch:index-document>
			<elasticsearch:update-document
				doc:name="Document - Update" doc:id="522e5969-e54c-4cb5-ae26-436d64126c39"
				config-ref="Elasticsearch_HTTP_Config3" index="update_doc_index_1"
				documentId="update_doc_id_1" jsonInputPath="${index.jsoninputpath}" >
				<elasticsearch:update-document-configuration routing="routing" timeoutInSec="6" ifSeqNo="22" ifPrimaryTerm="33" waitForActiveShards="55"
				refreshPolicy="WAIT_UNTIL" retryOnConflict="1" fetchSource="true" 
				detectNoop="false" scriptedUpsert="true" docAsUpsert="true"/>
			</elasticsearch:update-document>
			<elasticsearch:delete-document
