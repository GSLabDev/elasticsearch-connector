	<munit:test name="BulkDocumentWithTextFlowTest" description="Test"
		doc:id="87393dcb-e3fa-4ef5-8543-6a9055654c69">
		<munit:execution>
			<elasticsearch:bulk-operation doc:name="Document - Bulk"
			doc:id="cd0d94f0-f110-4396-910a-6a2ce2784dec" config-ref="Elasticsearch_HTTP_Config3">
			<elasticsearch:json-text>{ &quot;update&quot; : {&quot;_id&quot; : &quot;1&quot;, &quot;_index&quot; : &quot;accounts123&quot;, &quot;retry_on_conflict&quot; : 3} }
{ &quot;doc&quot; : {&quot;field&quot; : &quot;value&quot;} }
{ &quot;update&quot; : { &quot;_id&quot; : &quot;0&quot;, &quot;_index&quot; : &quot;accounts123&quot;, &quot;retry_on_conflict&quot; : 3} }
{ &quot;script&quot; : { &quot;source&quot;: &quot;ctx._source.counter += params.param1&quot;, &quot;lang&quot; : &quot;painless&quot;, &quot;params&quot; : {&quot;param1&quot; : 1}}, &quot;upsert&quot; : {&quot;counter&quot; : 1}}
{ &quot;update&quot; : {&quot;_id&quot; : &quot;2&quot;, &quot;_index&quot; : &quot;accounts123&quot;, &quot;retry_on_conflict&quot; : 3} }
{ &quot;doc&quot; : {&quot;field&quot; : &quot;value&quot;}, &quot;doc_as_upsert&quot; : true }
{ &quot;update&quot; : {&quot;_id&quot; : &quot;3&quot;, &quot;_index&quot; : &quot;accounts123&quot;, &quot;_source&quot; : true} }
{ &quot;doc&quot; : {&quot;field&quot; : &quot;value&quot;} }
{ &quot;update&quot; : {&quot;_id&quot; : &quot;4&quot;, &quot;_index&quot; : &quot;accounts123&quot;} }
{ &quot;doc&quot; : {&quot;field&quot; : &quot;value&quot;}, &quot;_source&quot;: true}
{ &quot;delete&quot; : {&quot;_id&quot; : &quot;5&quot;,  &quot;_index&quot; : &quot;accounts123&quot;} }
</elasticsearch:json-text>
		</elasticsearch:bulk-operation>
		
		<elasticsearch:delete-index doc:name="Delete index"
				doc:id="0a95fba3-3683-464f-85c1-6ee8806ebac8" config-ref="Elasticsearch_HTTP_Config3"
				index="${elasticsearch.index}" timeoutInSec="15"
				masterNodeTimeoutInSec="10">
				<elasticsearch:indices-opts
					ignoreUnavailable="true" allowNoIndices="true" expandWildcardsOpen="true"
					expandWildcardsClosed="true" allowAliasesToMultipleIndices="true"
					forbidClosedIndices="true" ignoreAliases="true" />
			</elasticsearch:delete-index>
		</munit:execution>
		<munit:validation>
			<munit-tools:assert-that doc:name="Assert that"
				doc:id="1b48966d-7d2b-4855-bd8b-739f357f0c5a" is="#[MunitTools::notNullValue()]"
				expression="#[payload]" />
		</munit:validation>
	</munit:test>

